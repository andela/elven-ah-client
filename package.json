{
  "name": "elven-ah-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "npm run build-css && react-scripts test --env=jsdom --coverage",
    "update:snap": "react-scripts test --env=jsdom --updateSnapshot",
    "eject": "react-scripts eject",
    "coverage": "react-scripts test --env=jsdom --coverage --coverageReporters=text-lcov | coveralls"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.jsx",
      "src/**/*.js",
      "src/**/**/*.jsx",
      "src/**/**/*.js"
    ],
    "coverageReporters": [
      "cobertura",
      "lcov",
      "html",
      "text",
      "text-summary"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "connected-react-router": "^4.4.1",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.5.0",
    "history": "^4.7.2",
    "moment": "^2.22.2",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "query-string": "^6.1.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "react-star-ratings": "^2.3.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "short-id": "0.1.0-1",
    "toastr": "^2.1.4"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "coveralls": "^3.0.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1"
  }
}
