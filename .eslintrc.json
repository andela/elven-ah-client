{
    "extends": "airbnb", 
    "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
      "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true,
          "modules": true
        }
      },
    "env": {
        "browser": true,
        "jasmine": true
      },
    "rules": {
        "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}],
        "no-underscore-dangle": ["error", { "allow": ["__REDUX_DEVTOOLS_EXTENSION__"] }]
    },
    "globals": {
        "window": true
    }
}
